{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [ "arn:aws:s3:::MyBucket"]
    },
    {
    "Effect": "Allow",
      "Action": [
	"s3:GetObjectAcl",
	"s3:GetObjectVersion",
	"s3:GetObject",
	"s3:GetObjectVersionAcl"
		],
      "Resource": [ "arn:aws:s3:::MyBucket/*"]
    }   
  ]
}
